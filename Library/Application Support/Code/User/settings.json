{
  "terminal.integrated.defaultProfile.linux": "zsh",
  "editor.find.seedSearchStringFromSelection": "selection",
  "editor.lightbulb.enabled": "off",
  "telemetry.telemetryLevel": "off",
  "editor.autoClosingBrackets": "never",
  "vim.leader": "<space>",
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.surround": true,
  "vim.replaceWithRegister": true,
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1,
    "asvetliakov.vscode-neovim": 1
  },
  "vscode-neovim.compositeKeys": {
    "jk": {
      "command": "vscode-neovim.escape"
    }
  },
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<ctrl>", "s"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "b"],
      "commands": [
        "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
      ]
    },
    {
      "before": ["<leader>", "<leader>"],
      "commands": ["extension.goto-previous-buffer"]
    },
    {
      "before": ["<ctrl>", "c"],
      "after": ["<Esc>"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["Y"],
      "after": ["v", "$", "h", "y"]
    },
    {
      "before": ["<leader>", "x"],
      "commands": [
        "workbench.action.files.save",
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "before": ["<leader>", "c", "l"],
      "commands": ["editor.action.commentLine"]
    },
    {
      "before": ["<leader>", "b", "d"],
      "commands": ["workbench.action.closeActiveEditor"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<leader>", "c"],
      "commands": ["editor.action.commentLine"]
    }
  ],
  "vim.commandLineModeKeyBindings": [],
  "settingsSync.ignoredExtensions": [],
  "editor.detectIndentation": true,
  "git.openRepositoryInParentFolders": "never",
  "settingsSync.ignoredSettings": [],
  "calva.paredit.defaultKeyMap": "strict",
  "calva.useLegacyReplWindowPath": false,
  "dev.containers.executeInWSL": true,
  "workbench.settings.applyToAllProfiles": [],
  "editor.fontLigatures": false,
  "go.toolsManagement.autoUpdate": true,
  "remote.autoForwardPortsSource": "hybrid",
  "java.debug.settings.exceptionBreakpoint.skipClasses": [],
  "dotnetAcquisitionExtension.enableTelemetry": false,
  "gitlens.telemetry.enabled": false,
  "stripe.telemetry.enabled": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "remote.localPortHost": "allInterfaces",
  "extensions.ignoreRecommendations": true,
  "python.languageServer": "Pylance",
  // "remote.SSH.useExecServer": false,
  // "remote.SSH.showLoginTerminal": true,
  // "remote.SSH.useLocalServer": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "editor.accessibilitySupport": "off",
  "github.copilot.editor.enableAutoCompletions": true,
  "editor.tabSize": 2,
  "makefile.configureOnOpen": true,
  "dart.flutterSdkPath": "/Users/tmalloy/development/flutter_sdk_3.22.3",
  "dart.openDevTools": "flutter",
  "[proto3]": {
    "editor.defaultFormatter": "zxh404.vscode-proto3"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.onTaskErrors": "abort",
  "vs-kubernetes": {
    "vs-kubernetes.crd-code-completion": "disabled"
  },
  "[clojure]": {
    "editor.defaultFormatter": "betterthantomorrow.calva",
    "editor.formatOnSave": true
  },
  // Why doesn't this override workspace settings?
  // "[go]": {
  //     "editor.formatOnSave": false
  // }
  // Setup fzf-picker extension
  "fzf-picker.customTasks": [
    // Choose folder to open on new window
    {
      "name": "zoxide",
      "command": "cursor $(zoxide query --interactive)"
    }
  ],
  // Allow top open a file with line number
  "fzf-picker.general.openCommand": "code -g",
  "chat.agent.enabled": true,
  "github.copilot.chat.agent.thinkingTool": true,
  "task.problemMatchers.neverPrompt": {
    "process": true
  },
  "github.copilot.advanced": {},
  "terminal.integrated.defaultProfile.osx": "zsh",
  "remote.SSH.remotePlatform": {
    "*.gitpod.local": "linux",
    "*.gitpod.remote": "linux"
  },
  "docker.extension.enableComposeLanguageServer": false,
  "terminal.integrated.scrollback": 50000,
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "diffEditor.maxComputationTime": 0
}
